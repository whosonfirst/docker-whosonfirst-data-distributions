#!/bin/sh
# -*-sh-*-

echo "Y U RUN ME... THIS IS PSEUDO-CODE"
exit 1

WOF_LIST_REPOS=`which wof-list-repos`
WOF_BUILD_DISTRIBUTIONS=`which wof-build-distributions`

GITHUB_ORGANIZATION="whosonfirst-data"
GITHUB_PREFIX="whosonfirst-data"

# getopts here...

TO_INDEX=`${WOF_LIST_REPOS} -organization ${GITHUB_ORGANIZATION} -prefix ${GITHUB_PREFIX} -since ${SINCE}`

${WOF_BUILD_DISTRIBUTIONS} ... ${TO_INDEX}

if [ $? -ne 0 ]
then
    echo "Failed to build combined distributions"
    exit 1
fi

exit 0
